
@prefix : <http://www.example.org/capstone#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

:CapstoneProject a owl:Class .

:Stakeholder a owl:Class .
:Student rdfs:subClassOf :Stakeholder .
:Supervisor rdfs:subClassOf :Stakeholder .
:Instructor rdfs:subClassOf :Stakeholder .
:Evaluator rdfs:subClassOf :Stakeholder .

:Role a owl:Class .
:ProjectManager rdfs:subClassOf :Role .
:TeamLeader rdfs:subClassOf :Role .
:Developer rdfs:subClassOf :Role .
:Tester rdfs:subClassOf :Role .
:Designer rdfs:subClassOf :Role .
:Advisor rdfs:subClassOf :Role .
:Evaluation rdfs:subClassOf :Role .

:Team a owl:Class .
:ProjectTeam rdfs:subClassOf :Team .

:ProjectPhase a owl:Class .
:InitiationPhase rdfs:subClassOf :ProjectPhase .
:PlanningPhase rdfs:subClassOf :ProjectPhase .
:ExecutionPhase rdfs:subClassOf :ProjectPhase .
:ClosurePhase rdfs:subClassOf :ProjectPhase .

:Deliverable a owl:Class .
:Documentation rdfs:subClassOf :Deliverable .
:ProjectProposal rdfs:subClassOf :Deliverable .
:RequirementsDocument rdfs:subClassOf :Deliverable .
:ProjectPlan rdfs:subClassOf :Deliverable .
:ProjectSchedule rdfs:subClassOf :Deliverable .
:StakeholderRegister rdfs:subClassOf :Deliverable .
:TestPlan_TestReport rdfs:subClassOf :Deliverable .
:FinalReport rdfs:subClassOf :Deliverable .
:UserManual rdfs:subClassOf :Deliverable .
:SoftwareSystem rdfs:subClassOf :Deliverable .
:Presentation rdfs:subClassOf :Deliverable .
:Poster rdfs:subClassOf :Deliverable .
:Prototype rdfs:subClassOf :Deliverable .

:Technology a owl:Class .
:ProgrammingLanguage rdfs:subClassOf :Technology .
:Framework rdfs:subClassOf :Technology .
:Database rdfs:subClassOf :Technology .
:SoftwareTool rdfs:subClassOf :Technology .

:Requirement a owl:Class .

:EvaluationCriterion a owl:Class .
:Innovation rdfs:subClassOf :EvaluationCriterion .
:Functionality rdfs:subClassOf :EvaluationCriterion .
:Timeliness rdfs:subClassOf :EvaluationCriterion .
:Viva rdfs:subClassOf :EvaluationCriterion .

:Risk a owl:Class .
:TechnicalRisk rdfs:subClassOf :Risk .
:ScheduleRisk rdfs:subClassOf :Risk .
:BudgetRisk rdfs:subClassOf :Risk .

:Challenge a owl:Class .
:TimeConstraint rdfs:subClassOf :Challenge .
:ResourceConstraint rdfs:subClassOf :Challenge .

:FutureScope a owl:Class .
:FutureWork rdfs:subClassOf :FutureScope .
:Extension rdfs:subClassOf :FutureScope .

:hasPhase a owl:ObjectProperty ;
    rdfs:domain :CapstoneProject ;
    rdfs:range :ProjectPhase .

:hasStakeholder a owl:ObjectProperty ;
    rdfs:domain :CapstoneProject ;
    rdfs:range :Stakeholder .

:hasRole a owl:ObjectProperty ;
    rdfs:domain :Stakeholder ;
    rdfs:range :Role .

:hasTeam a owl:ObjectProperty ;
    rdfs:domain :CapstoneProject ;
    rdfs:range :Team .

:isMemberOf a owl:ObjectProperty ;
    rdfs:domain :Stakeholder ;
    rdfs:range :Team .

:hasProjectManager a owl:ObjectProperty ;
    rdfs:domain :CapstoneProject ;
    rdfs:range :Stakeholder .

:managesProject a owl:ObjectProperty ;
    rdfs:domain :Stakeholder ;
    rdfs:range :CapstoneProject .

:hasDeliverable a owl:ObjectProperty ;
    rdfs:domain :CapstoneProject ;
    rdfs:range :Deliverable .

:producedBy a owl:ObjectProperty ;
    rdfs:domain :Deliverable ;
    rdfs:range :Stakeholder .

:usesTechnology a owl:ObjectProperty ;
    rdfs:domain :CapstoneProject ;
    rdfs:range :Technology .

:hasRequirement a owl:ObjectProperty ;
    rdfs:domain :CapstoneProject ;
    rdfs:range :Requirement .

:hasEvaluationCriterion a owl:ObjectProperty ;
    rdfs:domain :CapstoneProject ;
    rdfs:range :EvaluationCriterion .

:hasRisk a owl:ObjectProperty ;
    rdfs:domain :CapstoneProject ;
    rdfs:range :Risk .

:hasChallenge a owl:ObjectProperty ;
    rdfs:domain :CapstoneProject ;
    rdfs:range :Challenge .

:hasFutureScope a owl:ObjectProperty ;
    rdfs:domain :CapstoneProject ;
    rdfs:range :FutureScope .
