    rdfs:label "Documentation" ;
    rdfs:comment "A documentation-type deliverable (abstract class for project documents)." ;
    rdfs:subClassOf :Deliverable .
    

:ProjectManager a owl:Class ;
    rdfs:label "Project Manager" ;
    rdfs:comment "A role for a stakeholder who manages the project." ;
    rdfs:subClassOf :Role .
    

:TestPlan_TestReport a owl:Class ;
    rdfs:label "Test Plan/Test Report" ;
    rdfs:comment "A test plan and report document." ;
    rdfs:subClassOf :Documentation .
    

:Stakeholder a owl:Class ;
    rdfs:label "Stakeholder" ;
    rdfs:comment "A person involved in the capstone project." ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty :hasRole ],
        [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty :isMemberOf ],
        foaf:Person .
    
:TechnicalRisk a owl:Class ;
    rdfs:label "Technical Risk" ;
    rdfs:comment "A technical risk in the project." ;
    rdfs:subClassOf :Risk .
    


schema:ComputerLanguage a owl:Class ;
    rdfs:label "ComputerLanguage" ;
    rdfs:comment "This element represents a computer language. This element is reused from the Schema.org vocabulary." ;
    rdfs:subClassOf :Technology .
    


:Developer a owl:Class ;
    rdfs:label "Developer" ;
    rdfs:comment "A role for a stakeholder who develops the system." ;
    rdfs:subClassOf :Role .
    

:Team a owl:Class ;
    rdfs:label "Team" ;
    rdfs:comment "A team of stakeholders in the project." ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom xsd:integer ;
            owl:onProperty :teamSize ],
        schema:Organization .
    

:TimeConstraint a owl:Class ;
    rdfs:label "Time Constraint" ;
    rdfs:comment "A time-related constraint." ;
    rdfs:subClassOf :Challenge .
    

:Instructor a owl:Class ;
    rdfs:label "Instructor" ;
    rdfs:comment "A stakeholder who is an instructor of the course." ;
    rdfs:subClassOf :Stakeholder .
    

:CapstoneProject a owl:Class ;
    rdfs:label "Capstone Project" ;
    rdfs:comment "A planned capstone project with specific goals." ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty :hasTeam ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:integer ;
            owl:onProperty :teamSize ],
        [ a owl:Restriction ;
            owl:minCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty :hasDeliverable ],
        [ a owl:Restriction ;
            owl:minCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty :hasPhase ],
        [ a owl:Restriction ;
            owl:minCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty :hasStakeholder ],
        [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty :hasProjectManager ],
        schema:Project .
    

:FinalReport a owl:Class ;
    rdfs:label "Final Report" ;
    rdfs:comment "The final project report document." ;
    rdfs:subClassOf :Documentation .
    

schema:Organization a owl:Class ;
    rdfs:label "Organization" ;
    rdfs:comment "This element represents an organization. This element is reused from the Schema.org vocabulary." .
    

:ProjectSchedule a owl:Class ;
    rdfs:label "Project Schedule" ;
    rdfs:comment "A schedule/timeline document for the project." ;
    rdfs:subClassOf :Documentation .
    

:Presentation a owl:Class ;
    rdfs:label "Presentation" ;
    rdfs:comment "A presentation deliverable." ;
    rdfs:subClassOf schema:CreativeWork .
    
:BudgetRisk a owl:Class ;
    rdfs:label "Budget Risk" ;
    rdfs:comment "A budgetary risk in the project." ;
    rdfs:subClassOf :Risk .
    
:ProjectTeam a owl:Class ;
    rdfs:label "Project Team" ;
    rdfs:comment "A specific project team." ;
    rdfs:subClassOf :Team .
    

:Risk a owl:Class ;
    rdfs:label "Risk" ;
    rdfs:comment "A generic risk item in the project." .
    
:Database a owl:Class ;
    rdfs:label "Database" ;
    rdfs:comment "A database technology used in the project." ;
    rdfs:subClassOf :Technology .
    

:TeamLeader a owl:Class ;
    rdfs:label "Team Leader" ;
    rdfs:comment "A role for a stakeholder leading a team." ;
    rdfs:subClassOf :Role .
    

:Requirement a owl:Class ;
    rdfs:label "Requirement" ;
    rdfs:comment "A requirement for the project." .
    

:UserManual a owl:Class ;
    rdfs:label "User Manual" ;
    rdfs:comment "A user manual deliverable." ;
    rdfs:subClassOf :Documentation .
    

prov:Entity a owl:Class ;
    rdfs:label "Entity" ;
    rdfs:comment "This element represents an entity. This element is reused from the PROV-O ontology." .
    
:ProjectProposal a owl:Class ;
    rdfs:label "Project Proposal" ;
    rdfs:comment "A proposal document for the project." ;
    rdfs:subClassOf :Documentation .
    

:Deliverable a owl:Class ;
    rdfs:label "Deliverable" ;
    rdfs:comment "An artifact produced by the project (e.g. document or demo)." ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty :dueDate ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty :status ],
        [ a owl:Restriction ;
            owl:minCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty :producedBy ],
        schema:CreativeWork,
        prov:Entity .
    

:Tester a owl:Class ;
    rdfs:label "Tester" ;
    rdfs:comment "A role for a stakeholder who tests the system." ;
    rdfs:subClassOf :Role ;
    owl:disjointWith :EvaluationCriterion .

:Role a owl:Class ;
    rdfs:label "Role" ;
    rdfs:comment "A role or position that a stakeholder can assume." ;
    rdfs:subClassOf schema:Role ;
    owl:disjointWith :EvaluationCriterion .

:EvaluationCriterion a owl:Class ;
    rdfs:label "Evaluation Criterion" ;
    rdfs:comment "A criterion used to evaluate the project." .

