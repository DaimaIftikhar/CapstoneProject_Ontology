             rdfs:domain :Challenge ;
             rdfs:range xsd:string ;
             rdfs:comment "Identifier of the challenge." ;
             rdfs:label "challenge ID" .


###  http://example.org/capstone#confidentiality
:confidentiality rdf:type owl:DatatypeProperty ;
                 rdfs:domain :Documentation ;
                 rdfs:range xsd:string ;
                 rdfs:comment "Access level or privacy label of the document." ;
                 rdfs:label "confidentiality" .


###  http://example.org/capstone#contactNumber
:contactNumber rdf:type owl:DatatypeProperty ;
               rdfs:subPropertyOf schema:telephone ;
               rdfs:domain :Stakeholder ;
               rdfs:range xsd:string ;
               rdfs:comment "Contact telephone number of the stakeholder." ;
               rdfs:label "contact number" .


###  http://example.org/capstone#creationDate
:creationDate rdf:type owl:DatatypeProperty ;
              rdfs:subPropertyOf dcterms:created ;
              rdfs:domain :Documentation ;
              rdfs:range xsd:date ;
              rdfs:comment "The creation date of the document." ;
              rdfs:label "creation date" .


###  http://example.org/capstone#criterionName
:criterionName rdf:type owl:DatatypeProperty ;
               rdfs:subPropertyOf dcterms:title ;
               rdfs:domain :EvaluationCriterion ;
               rdfs:range xsd:string ;
               rdfs:comment "Name of the evaluation criterion." ;
               rdfs:label "criterion name" .


###  http://example.org/capstone#dbType
:dbType rdf:type owl:DatatypeProperty ;
        rdfs:domain :Database ;
        rdfs:range xsd:string ;
        rdfs:comment "Type of the database (e.g. SQL/NoSQL)." ;
        rdfs:label "database type" .


###  http://example.org/capstone#deliverableName
:deliverableName rdf:type owl:DatatypeProperty ;
