#################################################################
#    Classes
#################################################################

###  http://example.org/capstone#Advisor
:Advisor rdf:type owl:Class ;
         rdfs:subClassOf :Role ;
         rdfs:comment "A role for a stakeholder who advises the team." ;
         rdfs:label "Advisor" .


###  http://example.org/capstone#BudgetRisk
:BudgetRisk rdf:type owl:Class ;
            rdfs:subClassOf :Risk ;
            rdfs:comment "A budgetary risk in the project." ;
            rdfs:label "Budget Risk" .


###  http://example.org/capstone#CapstoneProject
:CapstoneProject rdf:type owl:Class ;
                 rdfs:subClassOf schema:Project ,
