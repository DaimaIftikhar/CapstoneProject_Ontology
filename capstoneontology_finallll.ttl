@prefix : <http://example.org/capstone#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <http://schema.org/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@base <http://example.org/capstone#> .

[ rdf:type owl:Ontology ;
   dcterms:creator "Hadia and Team"@en ;
   owl:versionInfo "1.0"@en
 ] .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/terms/creator
dcterms:creator rdf:type owl:AnnotationProperty .


#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2001/XMLSchema#date
xsd:date rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  http://example.org/capstone#hasChallenge
:hasChallenge rdf:type owl:ObjectProperty ;
              rdfs:domain :CapstoneProject ;
              rdfs:range :Challenge ;
              rdfs:comment "Links a project to a challenge/constraint." ;
              rdfs:label "has challenge" .


###  http://example.org/capstone#hasDeliverable
:hasDeliverable rdf:type owl:ObjectProperty ;
                rdfs:domain :CapstoneProject ;
                rdfs:range :Deliverable ;
                rdfs:comment "Links a project to one of its deliverables." ;
                rdfs:label "has deliverable" .


###  http://example.org/capstone#hasEvaluationCriterion
:hasEvaluationCriterion rdf:type owl:ObjectProperty ;
                        rdfs:domain :CapstoneProject ;
                        rdfs:range :EvaluationCriterion ;
                        rdfs:comment "Links a project to an evaluation criterion." ;
                        rdfs:label "has evaluation criterion" .


###  http://example.org/capstone#hasFutureScope
:hasFutureScope rdf:type owl:ObjectProperty ;
                rdfs:domain :CapstoneProject ;
                rdfs:range :FutureScope ;
                rdfs:comment "Links a project to a future work item." ;
                rdfs:label "has future scope" .


###  http://example.org/capstone#hasPhase
:hasPhase rdf:type owl:ObjectProperty ;
          rdfs:domain :CapstoneProject ;
          rdfs:range :ProjectPhase ;
          rdfs:comment "Links a project to one of its phases." ;
          rdfs:label "has phase" .


###  http://example.org/capstone#hasProjectManager
:hasProjectManager rdf:type owl:ObjectProperty ;
                   rdfs:domain :CapstoneProject ;
                   rdfs:range :Stakeholder ;
                   rdfs:comment "Links a project to its project manager." ;
                   rdfs:label "has project manager" .


###  http://example.org/capstone#hasRequirement
:hasRequirement rdf:type owl:ObjectProperty ;
                rdfs:domain :CapstoneProject ;
                rdfs:range :Requirement ;
                rdfs:comment "Links a project to a requirement." ;
                rdfs:label "has requirement" .


###  http://example.org/capstone#hasRisk
:hasRisk rdf:type owl:ObjectProperty ;
         rdfs:domain :CapstoneProject ;
         rdfs:range :Risk ;
         rdfs:comment "Links a project to a risk item." ;
         rdfs:label "has risk" .


###  http://example.org/capstone#hasRole
:hasRole rdf:type owl:ObjectProperty ;
         rdfs:domain :Stakeholder ;
         rdfs:range :Role ;
         rdfs:comment "Links a stakeholder to one of their roles." ;
         rdfs:label "has role" .


###  http://example.org/capstone#hasStakeholder
:hasStakeholder rdf:type owl:ObjectProperty ;
                rdfs:domain :CapstoneProject ;
                rdfs:range :Stakeholder ;
                rdfs:comment "Links a project to its stakeholders." ;
                rdfs:label "has stakeholder" .


###  http://example.org/capstone#hasTeam
:hasTeam rdf:type owl:ObjectProperty ;
         rdfs:domain :CapstoneProject ;
         rdfs:range :Team ;
         rdfs:comment "Links a project to its team." ;
         rdfs:label "has team" .


###  http://example.org/capstone#isMemberOf
:isMemberOf rdf:type owl:ObjectProperty ;
            rdfs:domain :Stakeholder ;
            rdfs:range :Team ;
            rdfs:comment "Indicates that a stakeholder is a member of a team." ;
            rdfs:label "is member of" .


###  http://example.org/capstone#managesProject
:managesProject rdf:type owl:ObjectProperty ;
                rdfs:domain :Stakeholder ;
                rdfs:range :CapstoneProject ;
                rdfs:comment "Inverse of hasProjectManager; stakeholder manages a project." ;
                rdfs:label "manages project" .


###  http://example.org/capstone#producedBy
:producedBy rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf prov:wasAttributedTo ;
            rdfs:domain :Deliverable ;
            rdfs:range :Stakeholder ;
            rdfs:comment "Associates a deliverable with its creator (stakeholder)." ;
            rdfs:label "produced by" .


###  http://example.org/capstone#usesTechnology
:usesTechnology rdf:type owl:ObjectProperty ;
                rdfs:domain :CapstoneProject ;
                rdfs:range :Technology ;
                rdfs:comment "Indicates a technology used in the project." ;
                rdfs:label "uses technology" .


###  http://www.w3.org/ns/prov#wasAttributedTo
prov:wasAttributedTo rdf:type owl:ObjectProperty ;
                     rdfs:comment "This element represents the attribution relationship. This element is reused from the PROV-O ontology." ;
                     rdfs:label "wasAttributedTo" .


#################################################################
#    Data properties
#################################################################

###  http://example.org/capstone#budget
:budget rdf:type owl:DatatypeProperty ;
        rdfs:domain :CapstoneProject ;
        rdfs:range xsd:decimal ;
        rdfs:comment "The budget for the project." ;
        rdfs:label "budget" .


###  http://example.org/capstone#challengeID
:challengeID rdf:type owl:DatatypeProperty ;
